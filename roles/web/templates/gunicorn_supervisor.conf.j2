[program:gunicorn_{{app_name}}]
command={{virtualenv_dir}}/bin/gunicorn -c {{app_code_dir}}/gunicorn_config.py {{app_name}}.wsgi
autostart=true
autorestart=true
stdout_logfile=/var/log/supervisor/supervisor-gunicorn-{{app_name}}-stdout.log
stderr_logfile=/var/log/supervisor/supervisor-gunicorn-{{app_name}}-stderr.log
